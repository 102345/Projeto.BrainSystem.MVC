@model BrainSystem.OS.MVC.ViewModels.RetiradaEquipamentosViewModel

<div class="modal-dialog animated rotateInDownRight">
    <div class="modal-content">
        <div class="modal-header">
            <button aria-hidden="true" data-dismiss="modal" class="close" type="button" onclick="AbrirURLEspecial('/CadastroPecasEqpto/Index/')">×</button>
            <h4 class="modal-title">Retirada Equipamento</h4>
        </div>
        <div class="modal-body">

            @using (Html.BeginForm("GravarRetiradaEquipamento", "CadastroPecasEqpto", FormMethod.Post, new { enctype = "multipart/form-data", role = "form", id = "frmCadastroRetiradaEqpto", @class = "form-horizontal" }))
            {
                @Html.AntiForgeryToken()
                @Html.HiddenFor(model => model.IdProdutoFalhado, new { @id = "hdIdProdutoFalhado" })
                <div class="form-group">
                    <label for="cboProduto" class="form-label">Produto</label>
                    @Html.DropDownListFor(model => model.IdProduto, Model.Produtos, new { @class = "form-control m-bot15", @id = "cboProduto" })
                </div>

                
                <div class="form-group">
                    <div class="input-group primary">
                        <span class="input-group-addon">
                            <span class="arrow"></span>
                            <i class="fa fa-list-ol"></i>
                        </span>
                        @Html.TextBoxFor(model => model.NumeroSerie, new { @class = "form-control", @placeholder = "Número Série", @id = "txtNroSerie" })
                    </div>
                </div>

                <div class="form-group">
                    <button type="submit" class="btn btn-primary pull-right" id="btnGravarRetiradaEqpto">OK</button>
                </div>

            }
        </div>
    </div>
</div>

@Scripts.Render("~/bundles/assets/js")

<script src="~/assets/js/jquery-1.11.2.min.js" type="text/javascript"></script>
<script src="~/assets/js/jquery.easing.min.js" type="text/javascript"></script>
<script src="~/assets/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>


<script src="~/assets/js/OrdemServico/CadastroPecasEqptos.js"></script>

