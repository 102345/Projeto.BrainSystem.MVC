@model BrainSystem.OS.MVC.ViewModels.Produtos
@*<div aria-hidden="true" role="dialog" tabindex="-1" id="modCadastroProdutosFalhados" class="modal fade" style="display: none;">*@
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button aria-hidden="true" data-dismiss="modal" class="close" type="button">×</button>
                <h4 class="modal-title">Produto Falhado</h4>
            </div>
            <div class="modal-body">

                @using (Html.BeginForm("GravarProdutoFalhado", "FluxoAtendimento", FormMethod.Post, new { enctype = "multipart/form-data", role = "form", id = "frmCadastroProdutoFalhado", @class = "form-horizontal", @onsubmit = "return IniciarLoader()" }))
                {
                    @Html.AntiForgeryToken()

                    <div class="form-group">
                        <label for="cboProduto" class="form-label">Produto</label>
                         @Html.DropDownList("IdProduto", null, string.Empty, new { @class = "form-control m-bot15", @onchange = "FiltrarLocalizacaoFalha($('#IdProduto'))" })
                    </div>
                    <div class="form-group">
                        <label for="txtNroSerie" class="form-label">Número Série</label>
                        @Html.TextBoxFor(model => model.NumeroSerie, new { @class = "form-control", @placeholder="Número Série" })
                    </div>
                    @*<div class="form-group" id="divComboLocalizacaoFalha">
                      
                    </div>
                    <div class="form-group" id="divComboDetalhamentoFalha">
                       
                    </div>
                    <div class="form-group" id="divComboAcaoCorretiva">
                        
                    </div>
                    <div class="form-group" id="divComboSolucaoAdotada">
                       
                    </div>*@
                    <div class="">
                        <label>
                            @Html.CheckBoxFor(model => model.StatusFuncionamento, new { @class = "iCheck" }) Status Funcionamento
                        </label>
                        
                    </div>
                    <button type="submit" class="btn btn-primary" id="btnGravarProdutoFalhado">OK</button>
                  }
            </div>
        </div>
    </div>
@*</div>*@
