    <script type="text/javascript">
          jQuery(document).ready(function ()
            {
                var b = jQuery('#Assinatura').val();
                var a = $('#divSigPad').signaturePad({ drawOnly: true, validateFields: false, lineTop: 180, output: '#Assinatura', canvas: '#xCanvas' });
                if (b !='' && a!= null) {

                    a.regenerate(b);
                }
            });
    </script>


