            <div class="page-sidebar-wrapper" id="main-menu-wrapper">

                <!-- USER INFO - START -->
                <div class="profile-info row">

                    <div class="profile-image col-xs-4">
                        <a href="ui-profile.html">
                            <img alt="" src="~/data/profile/profile.jpg" class="img-responsive img-circle">
                        </a>
                    </div>

                    <div class="profile-details col-xs-8">

                        <h3>
                            <a href="ui-profile.html">@ViewBag.NomeUsuario</a>

                            <!-- Available statuses: online, idle, busy, away and offline -->
                            <span class="profile-status online"></span>
                        </h3>

                        <p class="profile-title">@ViewBag.Cargo</p>

                    </div>

                </div>
                <!-- USER INFO - END -->



                <ul class='wraplist'>
                    <li class='menusection'>Atendimento</li>
                    <li class="">
                        <a href="javascript:;">
                            <i class="fa fa-edit"></i>
                            <span class="title">Ordens de serviço</span>
                            <span class="arrow "></span>
                        </a>
                        <ul class="sub-menu">
                            <li>
                                <a class="" href="/OrdemServico/SimulaOSFechada">Simula OS aberta/fechada</a>
                            </li>
                            <li>
                                <a class="" href="/OrdemServico/Index">OS Atual</a>
                            </li>
                        </ul>
                    </li>
                </ul>

                <div class="menustats">
                    <h5>Progresso do Atendimento</h5>
                    <div class="progress">
                        <div class="progress-bar progress-bar-primary" role="progressbar" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100" style="width: 50%;">
                        </div>
                    </div>
                </div>

            </div>
            <!-- MAIN MENU - END -->



        </div>
        <!--  SIDEBAR - END -->
        <!-- START CONTENT -->
        

        @RenderBody()
        <!-- END CONTENT -->
        
